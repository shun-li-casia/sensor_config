<launch>
<arg name="uav_id" default="0" />
<arg name="imu_uart" default="0" />
<arg name="camera_id" default="0" />
<arg name="if_gray" default="1" />
<node pkg="sensor_config" name="open_camera_split_imu_to_rosmsg_$(arg uav_id)" type="open_camera_split_imu_to_rosmsg" output="screen" ns="uav_$(arg uav_id)" args="-u $(arg uav_id) -i $(arg imu_uart) -c $(arg camera_id) -g $(arg if_gray)">
</node>
</launch>
